// create Route configuration
var clientCertPem = configurationMap.get(&quot;clientCertPem&quot;);
var clientKeyPem = configurationMap.get(&quot;clientKeyPem&quot;);
var trustStorePath = configurationMap.get(&quot;trustStorePath&quot;);
var trustStorePassword = configurationMap.get(&quot;trustStorePassword&quot;);

var route = new Route();
var ca = ClientCertAuthentication.fromCertAndKeyInPemFormat(clientCertPem, clientKeyPem);
route.setAuthentication(ca);
route.setChunkSize(0);
route.setCpa(&quot;cpaId&quot;);
route.setHost(&quot;localhost&quot;);
route.setName(&quot;route70&quot;);
route.setPartyId(&quot;partyId1&quot;);
route.setPath(&quot;/receiver&quot;);
route.setPort(9493);
route.setProtocol(&quot;https&quot;);
route.setRetry(0);
route.setTimeout(0);
route.setTrustStore(trustStorePath);
route.setTrustStorePassword(trustStorePassword);

// create and open connection
conn = new EbXmlRouteConnection(route);
conn.open();

// create ebXML request and send
var service = &quot;defaultservice&quot;;
var action = &quot;defaultaction&quot;;
var recordId = connectorMessage.getMessageId();
var arguments = &quot;&quot;;
var messageRecipient = &quot;test-server&quot;;
var certificateUrl = &quot;&quot;;
var rawMessage = connectorMessage.getRawData();
var req = new EbXmlRequest(service, action, recordId, arguments, messageRecipient, certificateUrl, rawMessage);
var resp = conn.send(req);

if(resp == null || !resp.ok()) {
	throw &quot;invalid response&quot;;
}

// close the connection
conn.close();

return resp;</script>
